{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "App0": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app0",
      "sourceRoot": "projects/app0/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/App0",
            "index": "projects/app0/src/index.html",
            "main": "projects/app0/src/main.ts",
            "polyfills": "projects/app0/src/polyfills.ts",
            "tsConfig": "projects/app0/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/app0/src/favicon.ico",
              "projects/app0/src/assets"
            ],
            "styles": [
              "projects/app0/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/app0/src/environments/environment.ts",
                  "with": "projects/app0/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "App0:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "App0:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "App0:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/app0/src/test.ts",
            "polyfills": "projects/app0/src/polyfills.ts",
            "tsConfig": "projects/app0/tsconfig.spec.json",
            "karmaConfig": "projects/app0/karma.conf.js",
            "assets": [
              "projects/app0/src/favicon.ico",
              "projects/app0/src/assets"
            ],
            "styles": [
              "projects/app0/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/app0/tsconfig.app.json",
              "projects/app0/tsconfig.spec.json",
              "projects/app0/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/app0/e2e/protractor.conf.js",
            "devServerTarget": "App0:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "App0:serve:production"
            }
          }
        }
      }
    },
